@model ContentfulMVC.Models.ViewModels.WhoIAm
<div id="wiaHeader" style="background:url('@Model.Pages.FeaturedImage.File.Url');background-size:cover;">

</div>
@Html.Partial("Wrapper")
<div class="row">
    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12" style="padding:0;">
        <img class="hImage" src="@Model.Pages.Image1.File.Url" />
    </div>
    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
        <h1>About Me</h1>
        <p class="contentfulContent">@Model.Author.Bio</p>
    </div>
</div>
<div class="row">
    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
        <h1>History</h1>
        <p class="contentfulContent">@Model.Author.Backstory</p>
    </div>
    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12" style="padding:0;">
        <img class="hImage" src="@Model.Pages.Image2.File.Url" />
    </div>
</div>
<div class="row">
    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12" style="padding:0;">
        <img class="hImage" src="@Model.Pages.Image3.File.Url" />
        <contentful-image asset-id="5na3PJ1om4eyAQcmia0S8y" />
        

    </div>
    <div id="wiaHobbies" class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
        <h1>Things I Enjoy Doing</h1>
        @foreach(string hobbie in Model.Author.Hobbies)
        {
            int i = 0;
            if (i <= Model.Author.Hobbies.Count / 2)
            {
                <div class="col-lg-6 col-md-6 col-sm-12">
                    <ul>
                        <li>@hobbie</li>
                    </ul>
                </div>
                i++;
            }
            else
            {
                <div class="col-lg-6 col-md-6 col-sm-12">
                    <ul>
                        <li>@hobbie</li>
                    </ul>
                </div>

            }


        }
    </div>
</div>
<h2>What is left to do on this page</h2>
<ul>
    <li>Add images via the image api</li>
    <li>Style them</li>
    <li>Style the page</li>
    <li>fix markdown for multiple entries</li>
</ul>
